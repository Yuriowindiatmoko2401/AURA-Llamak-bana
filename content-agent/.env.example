# Telegram Bot Configuration
# Get your bot token by messaging @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Get your chat_id by messaging @userinfobot on Telegram
# Note: This should be a user chat_id (positive number), group chat_id (negative number), or channel username (starting with @)
# Do NOT use another bot's chat_id - bots cannot message other bots
TELEGRAM_CHAT_ID=your_chat_id_here

# AI Model Configuration
# Choose your primary AI model: "gemini", "zai", or "deepseek"
CORE_AGENT_TYPE=deepseek

# Google Gemini API Key (if using Gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# ZAI API Key (if using ZAI)
ZAI_API_KEY=your_zai_api_key_here

# Deepseek API Key (if using Deepseek - uses REPLICATE_API_KEY)
# Note: Deepseek uses Replicate API, so make sure REPLICATE_API_KEY is set below
DEEPSEEK_API_KEY=your_replicate_token_here

# Image Generation (Replicate)
REPLICATE_API_KEY=your_replicate_token_here
REPLICATE_MODEL=ideogram-ai/ideogram-v3-turbo

# Other optional settings
DEBUG=false